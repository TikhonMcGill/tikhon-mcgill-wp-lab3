<%- include('header'); -%>

<div class="content">
  <h1>Selected Client</h1>
  <table class="table">
      <thead>
          <tr>
              <th>Client Name</th>
              <th scope="col">Society</th>
              <th scope="col">Address</th>
              <th scope="col">Zipcode</th>
              <th scope="col">City</th>
              <th scope="col">Phone</th>
              <th scope="col">Fax</th>
              <th scope="col">Max Outstanding</th>
          </tr>
      </thead>
      <tbody>
              <tr>
                  <th scope="row">
                      <%= clientus.contact %>
                  </th>
                  <th>
                      <%= clientus.society %>
                  </th>
                  <td>
                      <%= clientus.addres %>
                  </td>
                  <td>
                      <%= clientus.zipcode %>
                  </td>
                  <td>
                    <%= clientus.city %>
                  </td>
                  <td>
                    <%= clientus.phone %>
                  </td>
                  <td>
                    <%= clientus.fax %>
                  </td>
                  <td>
                    <%= clientus.max_outstanding %>
                  </td>
              </tr>

      </tbody>
  </table>
</div>
<br>
<%- include('footer'); -%>